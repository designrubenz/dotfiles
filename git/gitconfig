[core]
	excludesfile = /Users/johannes/.gitignore
  editor = /usr/bin/vim
[color]
  ui = auto
[alias]
  st = status
  c = commit --verbose
  co = checkout
  b = branch
  di = diff
  ds = diff --staged
  aa = add --all
  ff = merge --ff-only
  pullff = pull --ff-only
  noff = merge --no-ff
  f = fetch
  fa = fetch --all
  pom = push origin master

  plog = log --oneline --graph --all --abbrev-commit --decorate
  clog = log -p
  olog = log --oneline
[push]
  default = simple
